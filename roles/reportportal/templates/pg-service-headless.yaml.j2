---
apiVersion: v1
kind: Service
metadata:
  labels:
    app: postgresql
  name: postgresql-headless
  namespace: {{ meta.namespace }}
spec:
  clusterIP: None
  ports:
  - name: tcp-postgresql
    port: {{ rp_db_port }}
    protocol: TCP
    targetPort: tcp-postgresql
  selector:
    app: postgresql
  type: ClusterIP
